@using LivrETS.ViewModels
@using LivrETS.Models
@model ArticleViewModel
@{
    ViewBag.Title = "Vendre";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="page-header">
    <h1>Vendre un article <small>Remplissez tous les champs</small></h1>
</div>

@using (Html.BeginForm("Sell", "Index", FormMethod.Post, new { @class = "form" }))
{
    @Html.AntiForgeryToken()

    <div class="row">
        @Html.TextBoxFor(model => model.ISBN, new { @class = "" })
        <div class="input-group">
        </div>
    </div>
}
